{
  "name": "monitor-ts",
  "version": "1.0.0",
  "description": "Monitoramento de sites com Express e TypeScript",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'tsx' src/index.ts",
    "dev": "concurrently \"npm run start:dev\" \"cd resources && npm run dev\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "hiyan",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.4",
    "autoprefixer": "^10.4.20",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
